{
  "name": "vidtracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "purp test",
    "build:get-icons": "purp bundle -m GetIcons -t get-icons.js",
    "build:be": "purp bundle -m Main -t index.js",
    "build:fe": "purp bundle -m FrontEnd -t output/build/index.js && npm run build:parcel",
    "build:parcel": "parcel build output/build/index.js -d dist",
    "build:generate-css": "purp bundle -m GenerateStylesheet -t generate-css.js && node generate-css.js",
    "build": "purp build && npm run build:fe && npm run build:be && npm run build:get-icons && npm run build:generate-css"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "node-fetch": "^2.2.0",
    "open": "^0.0.5",
    "prettier": "^1.13.7",
    "puppeteer": "^1.6.1",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.9.7"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=3.0.0"
  },
  "alias": {
    "echarts": "./src/echarts.js"
  }
}
